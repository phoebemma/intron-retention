	shortjournal = {Physiol Genomics},
	author = {Lavin, Kaleen M. and Bell, Margaret B. and {McAdam}, Jeremy S. and Peck, Bailey D. and Walton, R. Grace and Windham, Samuel T. and Tuggle, S. Craig and Long, Douglas E. and Kern, Philip A. and Peterson, Charlotte A. and Bamman, Marcas M.},
