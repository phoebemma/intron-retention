	pmid = {29124498},
	pmcid = {PMC5846971},
	note = {Place: Netherlands},
	keywords = {Exercise, Female, Humans, Resistance Training/*methods, Aged, Male, Time Factors, Aging, Quality of life, Coping, Depression/*psychology, Mood, Quality of Life/*psychology, Sense of Coherence/*physiology, Well-being},
